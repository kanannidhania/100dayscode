Day 32 Question 1
#include <stdio.h>

int main() {
    int n1, n2;

    scanf("%d", &n1);
    int arr1[n1];
    for (int i = 0; i < n1; i++)
        scanf("%d", &arr1[i]);

    scanf("%d", &n2);
    int arr2[n2];
    for (int i = 0; i < n2; i++)
        scanf("%d", &arr2[i]);

    int merged[n1 + n2];
    for (int i = 0; i < n1; i++)
        merged[i] = arr1[i];
    for (int i = 0; i < n2; i++)
        merged[n1 + i] = arr2[i];

    for (int i = 0; i < n1 + n2; i++)
        printf("%d ", merged[i]);

    printf("\n");
    return 0;
}

Day 32 Question 2
#include <stdio.h>

int main() {
    long num;
    int count[10] = {0}, max = 0, digit, most = 0;

    scanf("%ld", &num);

    if (num < 0)
        num = -num;

    while (num > 0) {
        digit = num % 10;
        count[digit]++;
        num /= 10;
    }

    for (int i = 0; i < 10; i++) {
        if (count[i] > max) {
            max = count[i];
            most = i;
        }
    }

    printf("Digit %d occurs the most (%d times)\n", most, max);

    return 0;
}
