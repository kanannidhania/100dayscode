Day 25 Question 1
#include <stdio.h>

int main() {
    int i, j;

    for (i = 5; i >= 1; i--) {         
        for (j = i; j <= 5; j++) {     
            printf("%d", j);
        }
        printf("\n");
    }

    return 0;
}
Day 25 Queestion 2
#include <stdio.h>

int main() {
    int i, j;

    for (i = 5; i >= 1; i--) {         
        for (j = i; j <= 5; j++) {     
            printf("%d", j);
        }
        printf("\n");
    }

    return 0;
}

Day 25 Question 2
#include <stdio.h>

int main() {
    int i, j, spaces;

    for (i = 5; i >= 1; i--) {              
        spaces = 5 - i;                     
        for (j = 1; j <= spaces; j++) {    
            printf(" ");
        }

        for (j = 1; j <= i; j++) {
            printf("*");
        }

        printf("\n");
    }

    return 0;
}
