Day 24 question 1
#include <stdio.h>

int main() {
    int i, j;

    for (i = 1; i <= 5; i++) {      
        for (j = 1; j <= i; j++) {    
            printf("*");
        }
        printf("\n");
    }

    return 0;
}

Day 24 Question 2
#include <stdio.h>

int main() {
    int i, j;

    for (i = 1; i <= 5; i++) {       
        for (j = 1; j <= i; j++) {   
            printf("%d", j);
        }
        printf("\n");
    }

    return 0;
}
