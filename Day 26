Day 26 Question 1
#include <stdio.h>

int main() {
    int i, j, space;

    for (i = 5; i >= 1; i--) {

        for (space = 1; space < i; space++) {
            printf(" ");
        }

        for (j = i; j <= 5; j++) {
            printf("%d", j);
        }

        printf("\n");
    }

    return 0;
}

Day 26 Question 2
#include <stdio.h>

int main() {
    printf("*\n\n");

    for (int i = 1; i <= 3; i++)
        printf("*\n");
    printf("\n");

  
    for (int i = 1; i <= 5; i++)
        printf("*\n");
    printf("\n");

    for (int i = 1; i <= 3; i++)
        printf("*\n");
    printf("\n");

     printf("*\n");

    return 0;
}
