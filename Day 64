#include <stdio.h>
#include <string.h>

int main() {
    char s[1000];
    fgets(s, sizeof(s), stdin);
    int n = strlen(s);
    if (s[n - 1] == '\n') {
        s[n - 1] = '\0';
        n--;
    }

    int maxLen = 0;

    for (int i = 0; i < n; i++) {
        int visited[256] = {0};
        int len = 0;
        for (int j = i; j < n; j++) {
            if (visited[(unsigned char)s[j]])
                break;
            visited[(unsigned char)s[j]] = 1;
            len++;
        }
        if (len > maxLen)
            maxLen = len;
    }

    printf("%d\n", maxLen);
    return 0;
}
