#include <stdio.h>
#include <string.h>

int main() {
    char s[1000], t[1000];
    fgets(s, sizeof(s), stdin);
    fgets(t, sizeof(t), stdin);

    int n1 = strlen(s), n2 = strlen(t);
    if (s[n1 - 1] == '\n') { s[n1 - 1] = '\0'; n1--; }
    if (t[n2 - 1] == '\n') { t[n2 - 1] = '\0'; n2--; }

    if (n1 != n2) {
        printf("Not Anagram\n");
        return 0;
    }

    int freq[26] = {0};

    for (int i = 0; i < n1; i++) {
        freq[s[i] - 'a']++;
        freq[t[i] - 'a']--;
    }

    for (int i = 0; i < 26; i++) {
        if (freq[i] != 0) {
            printf("Not Anagram\n");
            return 0;
        }
    }

    printf("Anagram\n");
    return 0;
}
